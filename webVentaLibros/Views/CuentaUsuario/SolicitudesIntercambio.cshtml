    <div class="main">
        <!-- MAIN CONTENT -->
        <div class="main-content">
            <div class="container-fluid">
                
                        <!-- INPUT GROUPS -->
                        <div class="panel">
                            <div class="panel-heading">
                                <h3 class="panel-title">SOLICITUDES DE INTERCAMBIO</h3>
                            </div>

                            <div class="panel-body">
                                <!--SOLICITUDES RECIBIDAS-->
                                <fieldset> <legend>Recibidas</legend>
                                    <div class="container">

                                        <div class="row">
                                            <div class="col-sm-12">
                                                <table id="example" class="display" cellspacing="0" width="100%">
                                                    <thead>
                                                        <tr>
                                                            <th></th>
                                                            <th>Mi libro</th>
                                                            <th>Genero</th>
                                                            <th></th>
                                                            <th>Libro ofrecido</th>
                                                            <th>Genero</th>
                                                            <th>Estado</th>
                                                            <th>Fecha</th>
                                                            <th>Ver</th>

                                                        </tr>
                                                    </thead>
                                                    <tfoot>
                                                        <tr>
                                                            <th></th>
                                                            <th>Mi libro</th>
                                                            <th>Genero</th>
                                                            <th></th>
                                                            <th>Libro ofrecido</th>
                                                            <th>Genero</th>
                                                            <th>Estado</th>
                                                            <th>Fecha</th>
                                                            <th>Ver</th>
                                                        </tr>
                                                    </tfoot>
                                                    <tbody>

                                                        @foreach (var intercambio in ViewBag.solicitudesIntercambioRecibidos)
                                                        {
                                                            <tr>
                                                                <td style="width: 50px; height: 70px; background-color:lightgoldenrodyellow"><img style="width:100%; height:100%" src="../@intercambio.PublicacionIntercambio.foto" /></td>
                                                                <td style="background-color:lightgoldenrodyellow">@intercambio.PublicacionIntercambio.titulo</td>
                                                                <td style="background-color:lightgoldenrodyellow">@intercambio.PublicacionIntercambio.Generos.nombre</td>
                                                                <td style="width: 50px; height: 70px; background-color:lightgreen"><img style="width:100%; height:100%" src="../@intercambio.PublicacionIntercambio1.foto" /></td>
                                                                <td style="background-color:lightgreen">@intercambio.PublicacionIntercambio1.titulo</td>
                                                                <td style="background-color:lightgreen">@intercambio.PublicacionIntercambio1.Generos.nombre</td>
                                                                <td>@intercambio.EstadosIntercambio.nombre</td>
                                                                <td>@intercambio.fechaHora</td>
                                                                <td>
                                                                    <form action=@Url.Action("SolicitudRecibida", "CuentaUsuario") enctype="multipart/form-data" method="GET" role="form">
                                                                        <input type="text" value="@intercambio.idPublicacionUsuario1" id="idPublicacion1" name="idPublicacion1" hidden />
                                                                        <input type="text" value="@intercambio.idPublicacionUsuario2" id="idPublicacion2" name="idPublicacion2" hidden />
                                                                        <button type="submit" class="btn btn-default btn-sm">
                                                                            <span class="glyphicon glyphicon-eye-open" aria-hidden="true">
                                                                            </span>
                                                                        </button>
                                                                    </form>
                                                                </td>
                                                            </tr>
                                                        }

                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </fieldset>
                                <!--FIN SOLICITUDES RECIBIDAS-->
                                <br />
                                <br />
                                <br />
